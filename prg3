int motor_pin1 = 8; //For clockwise directions 
int motor_pin2 = 9; //For anti- clockwise directions 
void setup() 
{ 
pinMode(motor_pin1, OUTPUT); 
pinMode(motor_pin2, OUTPUT); 
} 
void loop() 
{ 
digitalWrite(motor_pin1, HIGH); //For clockwise directions 
digitalWrite(motor_pin2, LOW); 
delay(6000); // wait for a second 
digitalWrite(motor_pin1, LOW); //For anti- clockwise directions 
digitalWrite(motor_pin2, HIGH); 
delay(6000); // wait for a second }
